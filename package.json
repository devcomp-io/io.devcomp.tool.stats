{
  "name": "io.devcomp.tool.stats",
  "version": "0.0.0",
  "private": true,
  "pm": "npm",
  "dependencies": {
    "mongodb": "^1.4.19",
    "node-statsd": "0.0.7",
    "smi.cli": "0.x",
    "waitfor": "^0.1.3"
  },
  "scripts": {
    "install": "./node_modules/.bin/smi install"
  },
  "upstream": {
    "packages": {
      "top": [
        "../../*",
        "../../../../*"
      ]
    }
  },
  "mappings": {
    "io.pinf.server.www": "top/io.pinf.server.www"
  },
  "config": {
    "smi.cli": {
      "packagesDirectory": "node_modules"
    },
    "pio.deploy.converter": {
      "name": "nodejs-server"
    }
  },
  "config.plugin": {
    "io.pinf.server.firewidgets": {
      "widgets": {
        "service": "./widgets/service/*"
      }
    }
  }
}
